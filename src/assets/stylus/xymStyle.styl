  /* 地图marker样式 */
  .point{
    position: relative;
    width: 48px;
    height: 48px;
    margin-left: -16px;
    margin-top: -24px;
  }
  .point-tip{
    position: absolute;
    top: -60px;
    left: 50%;
    margin-left: -67px;
    width: 130px;
    height: 50px;
    background: rgba(52,65,76,0.90);
    border-radius: 4px;
    display: none;
  }
  .point-tip.point-tip-show{
    display: block;
  }
  .point-tri{
    position: absolute;
    bottom: -18px;
    left: 50%;
    margin-left: -6px;
    width: 0; 
    height: 0;
    border-width: 9px;
    border-style: solid;
    border-color: rgba(52,65,76,0.90) transparent transparent transparent;
  }
  .point-tip-p{
    font-size: 12px;
    line-height: 20px;
    color: #FFFFFF;
    padding: 0 10px;
    white-space: normal;
    margin-top: -37px;
  }
  .point-light{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #4272FF;
    opacity: 0.5;
    border-radius: 100%;
    animation: myScale 1.5s infinite forwards;
  }
  .point-circle{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 12px;
    height: 12px;
    background: #4272FF;
    border-radius: 100%;
  }
  @keyframes myScale{
    0% {opacity: 0.8;transform: scale(0.1);}
    100% {opacity: 0;transform: scale(1);}
  }
  /* 地图marker样式结束 */


