// 全局
.dia-con-p{
  text-align: center;
}
.dia-con-p h4{
  font-size: 24px;
  color: #34414C;
  margin-top 10px;
}
.dia-con-p p{
  font-size: 14px;
  color: #7E8A95;
  margin-top 8px;
}
.dia-ul{
  list-style none;
  overflow: auto;
  width 430px;
  margin: 30px auto;
  max-height: 170px;
  padding: 0 20px;
}
.dia-ul li{
  float left
  width 50%;
  font-size: 14px;
  color: #7E8A95;
  line-height: 34px;
}
.dia-ul li:nth-child(n){
  text-align: right;
  padding: 0 10px 0 0 
}
.dia-ul li:nth-child(2n){
  text-align: left;
  padding: 0 0 0 10px
}
.dia-ul li.single{
  width 100% !important;
  text-align: center !important;
}



